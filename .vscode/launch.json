{
	"version": "0.1.0",
	"configurations": [{
		"type": "node",
		"request": "launch",
		"name": "Launch Program",
		"program": "${workspaceFolder}/dist/server.js",
		"stopOnEntry": false,
		"cwd": "${workspaceFolder}",
		"runtimeArgs": [
			"--nolazy"
		],
		"env": {
			"NODE_ENV": "development",
		},
		"sourceMaps": true,
		"outFiles": ["${workspaceFolder}/dist"],
	}]
}